<!--go cart drawer-->
<div class="go-cart__drawer js-go-cart-drawer">

    <div class="go-cart-drawer">

        <!--go cart drawer close-->
        <div class="go-cart-drawer__header">
            <div class="go-cart-drawer__title">
                <h3 class="title--sm fnt-sec uc md fw-500">Order summary</h3>
            </div>
            <div class="go-cart-drawer__close js-go-cart-drawer-close">

                {%- render 'icons', icon: 'close' -%}

            </div>

        </div>
        <!--end go cart drawer close-->

        <!--go cart drawer content-->
        <div class="go-cart-drawer__content js-go-cart-drawer-content"></div>
        <!--end go cart drawer content-->

        <!--go cart drawer subtotal-->
        <div class="go-cart-drawer__subtotal mt-md">
            <h3 class="title--sm fnt-sec md uc fw-500 pt-lg">Payment Summary</h3>
            <div class="go-cart-drawer__payment-details flex f-space-between fnt-sec py-sm sm uc fw-300">
                <span>Subtotal: </span>
                <span class="go-cart-drawer__subtotal-price js-go-cart-drawer-subtotal"></span>
            </div>
        </div>
        <!--end go cart drawer subtotal-->

        <!--go cart drawer buttons-->
        <div class="go-cart-drawer__footer js-go-cart-drawer-footer">

            {%- liquid
                assign link_text = 'cart.general.checkout' | t
                echo link_text | link_to: '/checkout/', class: 'btn btn--full sm'
            -%}

            {% comment %} <div class="go-cart-drawer__fast-checkout mt-md text-align-center">
                <h4 class="fnt-sec uc sm">Fast check out</h4>
            </div> {% endcomment %}
        {%- comment -%} 
            <a href="/cart/" class="go-cart__button">
                View cart
            </a>
        {%- endcomment -%}

        </div>
        <!--end go cart drawer buttons-->

    </div>

</div>
<!--end go cart drawer-->

<!--go cart overlay-->
<div class="go-cart__overlay js-go-cart-overlay">
</div>
